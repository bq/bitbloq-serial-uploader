{
    "name": "bitbloq Serial Uploader",
    "short_name": "bitbloqSU",
    "description": "Serial Uploader for bitbloq",
    "author": [
        "Sergio Morcuende <sergio.morcuende@bq.com>",
        "Irene Sanz <irene.sanz@bq.com>",
        "Diego Segura <diego.segura@bq.com>",
        "Laura Del Río <laura.delrio@bq.com>",
        "Abraham Sanchez <abraham.sanchez@bq.com>",
        "Fernando Martín <fernando.martin@bq.com>",
        "Anthanh Pham <anthanh.pham@bq.com>",
        "David Baena <david.baena@bq.com>"
    ],
    "version": "1.7.13",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "default_locale": "es",
    "permissions": ["serial"],
    "url_handlers": {
        "bitbloq": {
            "matches": [
                "matches_handler"
            ],
            "title": "bitbloqSU inline installation"
        }
    },
    "app": {
        "background": {
            "scripts": ["launch.js"]
        }
    },
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },
    "externally_connectable": {
        "matches": [
            "matches_externally_connectable"
        ]
    },
    "offline_enabled": true

}
